{
    "$schema": "https://typedoc.org/schema.json",
    "name": "WDIO Obsidian Service",
    "entryPointStrategy": "packages",
    "entryPoints": ["packages/*"],
    "out": "./generated-docs",
    "readme": "./README.md",
    "sourceLinkExternal": true,
    "githubPages": true,
    "searchInComments": true,
    "searchInDocuments": true,
    "navigationLinks": {
        "Example": "https://github.com/jesse-r-s-hines/wdio-obsidian-service-sample-plugin",
        "GitHub": "https://github.com/jesse-r-s-hines/wdio-obsidian-service"
    },
    "navigation": {
        "includeCategories": true
    },
    "visibilityFilters": {},
    "headings": {
        "readme": false,
        "document": false
    },
    "router": "structure",
    "projectDocuments": [
        "./README.md",
        "./CONTRIBUTING.md"
    ],
    "packageOptions": {
        "entryPoints": ["src/index.ts"],
        "readme": "none",
        "defaultCategory": "none",
        "categoryOrder": ["Options", "Utilities", "WDIO Helpers", "*", "Types"],
        "sort": [
            "kind",
            "instance-first",
            "source-order"
        ]
    }
}
