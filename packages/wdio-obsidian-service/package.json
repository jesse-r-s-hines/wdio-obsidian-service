{
    "name": "wdio-obsidian-service",
    "version": "2.2.1",
    "description": "A WebdriverIO service for end-to-end testing of Obsidian plugins",
    "keywords": [
        "obsidian",
        "webdriverio",
        "wdio",
        "wdio-service",
        "tests"
    ],
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": "./dist/index.js"
    },
    "files": [
        "dist",
        "helper-plugin",
        "default-vault"
    ],
    "scripts": {
        "build": "tsc && tsup",
        "test": "pnpm run \"/^test:.*(?<!:e2e)$/\"",
        "test:unit": "tsc -p ./test && mocha --config .mocharc.unit.json",
        "test:e2e": "pnpm run \"/^test:e2e:.*/\"",
        "test:e2e:wdio": "wdio run ./test/wdio.conf.ts",
        "test:e2e:standalone": "mocha --config .mocharc.standalone.json",
        "test:android": "wdio run ./test/wdio.mobile.conf.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jesse-r-s-hines/wdio-obsidian-service.git",
        "directory": "packages/wdio-obsidian-service"
    },
    "author": "jesse-r-s-hines",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jesse-r-s-hines/wdio-obsidian-service/issues"
    },
    "homepage": "https://jesse-r-s-hines.github.io/wdio-obsidian-service/wdio-obsidian-service/README.html",
    "devDependencies": {
        "@types/chai": "~5.2.3",
        "@types/lodash": "~4.17.23",
        "@types/mocha": "~10.0.10",
        "@types/node": "^18.19.130",
        "@types/semver": "~7.7.1",
        "@wdio/appium-service": "^9.18.4",
        "@wdio/cli": "^9.18.1",
        "@wdio/globals": "^9.17.0",
        "@wdio/local-runner": "^9.18.1",
        "@wdio/logger": "^9.18.0",
        "@wdio/mocha-framework": "^9.18.0",
        "@wdio/spec-reporter": "^9.18.0",
        "@wdio/types": "^9.16.2",
        "chai": "~6.2.2",
        "cross-env": "~10.1.0",
        "mocha": "~10.8.2",
        "ts-node": "~10.9.2",
        "tsup": "~8.5.1",
        "tsx": "~4.21.0",
        "typescript": "~5.9.3",
        "wdio-obsidian-reporter": "2.2.1"
    },
    "dependencies": {
        "lodash": "~4.17.21",
        "obsidian-launcher": "2.2.1",
        "semver": "~7.7.3",
        "tar": "~7.5.3"
    },
    "peerDependencies": {
        "@wdio/cli": "^9.18.1",
        "@wdio/local-runner": "^9.18.1",
        "@wdio/logger": "^9.18.0",
        "@wdio/mocha-framework": "^9.18.0",
        "appium": ">=2.19.0",
        "appium-uiautomator2-driver": ">=4.2.7",
        "obsidian": "^1.8.7",
        "webdriverio": "^9.18.1"
    },
    "peerDependenciesMeta": {
        "appium": {
            "optional": true
        },
        "appium-uiautomator2-driver": {
            "optional": true
        }
    },
    "engines": {
        "node": ">=18.20.0"
    }
}
