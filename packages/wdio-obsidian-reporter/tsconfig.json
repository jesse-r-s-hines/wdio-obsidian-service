{
    "include": ["./**/*"],
    "compilerOptions": {
        /* Visit https://aka.ms/tsconfig to read more about this file */
        "noEmit": true,
        "target": "es2020",
        "lib": ["es2020", "dom"],
        "module": "node16",
        "moduleResolution": "node16",
        "types": ["node"],
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}
