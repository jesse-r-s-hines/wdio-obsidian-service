{
    "name": "obsidian-launcher",
    "version": "2.1.3",
    "description": "Download and launch sandboxed Obsidian instances",
    "keywords": [
        "obsidian"
    ],
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        "require": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.cjs"
        },
        "import": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "bin": {
        "obsidian-launcher": "./dist/cli.js"
    },
    "scripts": {
        "build": "tsc && tsup",
        "test": "pnpm run \"/^test:.*/\"",
        "test:unit": "mocha --config .mocharc.unit.json",
        "test:e2e": "mocha --config .mocharc.e2e.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jesse-r-s-hines/wdio-obsidian-service.git",
        "directory": "packages/obsidian-launcher"
    },
    "author": "jesse-r-s-hines",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jesse-r-s-hines/wdio-obsidian-service/issues"
    },
    "homepage": "https://jesse-r-s-hines.github.io/wdio-obsidian-service/obsidian-launcher/README.html",
    "devDependencies": {
        "@octokit/rest": "^22.0.0",
        "@types/chai": "^5.2.2",
        "@types/chrome-remote-interface": "^0.31.14",
        "@types/cross-spawn": "^6.0.6",
        "@types/lodash": "^4.17.20",
        "@types/mocha": "^10.0.10",
        "@types/node": "^18.15.3",
        "@types/readline-sync": "^1.4.8",
        "@types/semver": "^7.7.0",
        "@types/serve-handler": "^6.1.4",
        "chai": "^5.2.1",
        "mocha": "^11.7.1",
        "serve": "^14.2.4",
        "serve-handler": "^6.1.6",
        "ts-essentials": "^10.1.1",
        "ts-node": "^10.9.2",
        "tsup": "^8.5.0",
        "tsx": "^4.20.3",
        "typescript": "^5.9.2"
    },
    "dependencies": {
        "7z-wasm": "1.2.0",
        "@electron/get": "^3.1.0",
        "@supercharge/promise-pool": "^3.2.0",
        "chrome-remote-interface": "^0.33.3",
        "classic-level": "^3.0.0",
        "commander": "^13.1.0",
        "dotenv": "^17.2.1",
        "extract-zip": "^2.0.1",
        "lodash": "^4.17.21",
        "readline-sync": "^1.4.10",
        "semver": "^7.7.2"
    },
    "engines": {
        "node": ">=18"
    }
}
