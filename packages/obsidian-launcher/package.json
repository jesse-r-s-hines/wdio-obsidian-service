{
    "name": "obsidian-launcher",
    "version": "1.3.2",
    "description": "Download and launch sandboxed Obsidian instances",
    "keywords": [
        "obsidian"
    ],
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        "require": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.cjs"
        },
        "import": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "bin": {
        "obsidian-launcher": "./dist/cli.js"
    },
    "scripts": {
        "build": "tsup",
        "test": "run-s test:*",
        "test:unit": "tsc --noEmit && mocha --config .mocharc.unit.json",
        "test:e2e": "tsc --noEmit && mocha --config .mocharc.e2e.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jesse-r-s-hines/wdio-obsidian-service.git",
        "directory": "packages/obsidian-launcher"
    },
    "author": "jesse-r-s-hines",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jesse-r-s-hines/wdio-obsidian-service/issues"
    },
    "homepage": "https://jesse-r-s-hines.github.io/wdio-obsidian-service/obsidian-launcher/README.html",
    "devDependencies": {
        "@types/chai": "^5.0.1",
        "@types/chrome-remote-interface": "^0.31.14",
        "@types/lodash": "^4.17.14",
        "@types/mocha": "^10.0.10",
        "@types/node": "^18.11.9",
        "@types/semver": "^7.5.8",
        "chai": "^5.1.2",
        "mocha": "^11.0.1",
        "npm-run-all": "^4.1.5",
        "ts-node": "^10.9.2",
        "tsup": "^8.3.5",
        "tsx": "^4.19.2",
        "typescript": "^5.8.2"
    },
    "dependencies": {
        "@electron/get": "^3.1.0",
        "@supercharge/promise-pool": "^3.2.0",
        "chrome-remote-interface": "^0.33.2",
        "classic-level": "^2.0.0",
        "commander": "^13.0.0",
        "extract-zip": "^2.0.1",
        "lodash": "^4.17.21",
        "semver": "^7.6.3",
        "which": "^5.0.0"
    },
    "engines": {
        "node": ">=18"
    }
}
