{
    "name": "wdio-obsidian-service-workspace",
    "version": "2.0.2",
    "private": true,
    "bin": {},
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jesse-r-s-hines/wdio-obsidian-service.git"
    },
    "scripts": {
        "build": "pnpm run -r build",
        "release": "node scripts/release.mjs",
        "lint": "eslint packages/*/src packages/*/test/e2e packages/*/test/unit packages/*/test/standalone",
        "test": "pnpm run '/^test:.*/'",
        "test:unit": "pnpm run -r --if-present test:unit",
        "test:e2e": "pnpm run -r --if-present test:e2e",
        "test:android": "pnpm run -r --if-present test:android",
        "doc": "typedoc",
        "preview-doc": "pnpm run build && typedoc && cd generated-docs && serve"
    },
    "devDependencies": {
        "@eslint/js": "^9.32.0",
        "appium": "^2.19.0",
        "appium-uiautomator2-driver": "^4.2.7",
        "eslint": "^9.32.0",
        "serve": "^14.2.4",
        "typedoc": "^0.28.9",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.39.0"
    },
    "engines": {
        "node": ">=18"
    }
}
